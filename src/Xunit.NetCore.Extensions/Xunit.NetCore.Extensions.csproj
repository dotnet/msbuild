<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>$(LibraryTargetFrameworks)</TargetFrameworks>
    <CLSCompliant>false</CLSCompliant>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <IsShipping>false</IsShipping>
    <IsTestProject>false</IsTestProject><!-- while this project references xunit it isn't itself a test -->
  </PropertyGroup>
  <ItemGroup>
    <!-- xUnit v3 package for test framework extension -->
    <PackageReference Include="xunit.v3.core" VersionOverride="$(XUnitV3Version)" />
  </ItemGroup>
</Project>
