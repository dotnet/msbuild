<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>$(RuntimeOutputTargetFrameworks)</TargetFrameworks>
    <CLSCompliant>false</CLSCompliant>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <IsShipping>false</IsShipping>
    <IsTestProject>false</IsTestProject><!-- while this project references xunit it isn't itself a test -->
  </PropertyGroup>
  <ItemGroup>
    <!-- xUnit v3 extensibility package (not xunit.v3.core, which requires OutputType=Exe) -->
    <PackageReference Include="xunit.v3.extensibility.core" VersionOverride="$(XUnitV3Version)" />
  </ItemGroup>
</Project>
