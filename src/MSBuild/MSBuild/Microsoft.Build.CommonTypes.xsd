    <!-- ======================== DOTNET PACK PROPERTIES =====================================-->
    <xs:element name="AssemblyTitle" type="msb:StringPropertyType" substitutionGroup="msb:Property">
      <xs:annotation>
        <xs:documentation><!-- _locID_text="AssemblyTitle" _locComment="" -->Description for the assembly manifest</xs:documentation>
      </xs:annotation>
    </xs:element>
    <xs:element name="AssemblyVersion" type="msb:StringPropertyType" substitutionGroup="msb:Property">
      <xs:annotation>
        <xs:documentation><!-- _locID_text="AssemblyVersion" _locComment="" -->Numeric value of the version for the assembly manifest in the format major.minor.patch (e.g. 2.4.0)</xs:documentation>
      </xs:annotation>
    </xs:element>
    <xs:element name="Authors" type="msb:StringPropertyType" substitutionGroup="msb:Property">
      <xs:annotation>
        <xs:documentation><!-- _locID_text="Copyright" _locComment="" -->A comma-separated list of NuGet packages authors</xs:documentation>
      </xs:annotation>
    </xs:element>
    <xs:element name="Company" type="msb:StringPropertyType" substitutionGroup="msb:Property">
      <xs:annotation>
        <xs:documentation><!-- _locID_text="Company" _locComment="" -->Company name for the assembly manifest</xs:documentation>
      </xs:annotation>
    </xs:element>
    <xs:element name="Copyright" type="msb:StringPropertyType" substitutionGroup="msb:Property">
      <xs:annotation>
        <xs:documentation><!-- _locID_text="Copyright" _locComment="" -->Copyright details for the NuGet package</xs:documentation>
      </xs:annotation>
    </xs:element>
    <xs:element name="Description" type="msb:StringPropertyType" substitutionGroup="msb:Property">
      <xs:annotation>
        <xs:documentation><!-- _locID_text="Description" _locComment="" -->A long description of the NuGet package for UI display</xs:documentation>
      </xs:annotation>
    </xs:element>
    <xs:element name="FileVersion" type="msb:StringPropertyType" substitutionGroup="msb:Property">
      <xs:annotation>
        <xs:documentation><!-- _locID_text="FileVersion" _locComment="" -->Numeric value of the version for the assembly manifest in the format major.minor.build.revision (e.g. 2.4.0.1)</xs:documentation>
      </xs:annotation>
    </xs:element>
    <xs:element name="InformationalVersion" type="msb:StringPropertyType" substitutionGroup="msb:Property">
      <xs:annotation>
        <xs:documentation><!-- _locID_text="InformationalVersion" _locComment="" -->Product version of the assembly for UI display (e.g. 1.0 Beta)</xs:documentation>
      </xs:annotation>
    </xs:element>
    <xs:element name="NeutralLanguage" type="msb:StringPropertyType" substitutionGroup="msb:Property">
      <xs:annotation>
        <xs:documentation><!-- _locID_text="NeutralLanguage" _locComment="" -->The locale ID for the NuGet package</xs:documentation>
      </xs:annotation>
    </xs:element>
    <xs:element name="PackageIconUrl" type="msb:StringPropertyType" substitutionGroup="msb:Property">
      <xs:annotation>
        <xs:documentation><!-- _locID_text="PackageIconUrl" _locComment="" -->The URL for a 64x64 image with transparent background to use as the icon for the NuGet package in UI display</xs:documentation>
      </xs:annotation>
    </xs:element>
    <xs:element name="PackageId" type="msb:StringPropertyType" substitutionGroup="msb:Property">
      <xs:annotation>
        <xs:documentation><!-- _locID_textPackageId _locComment="" -->The case-insensitive NuGet package identifier, which must be unique across nuget.org or whatever gallery the NuGet package will reside in. IDs may not contain spaces or characters that are not valid for a URL, and generally follow .NET namespace rules.</xs:documentation>
      </xs:annotation>
    </xs:element>
    <xs:element name="Title" type="msb:StringPropertyType" substitutionGroup="msb:Property">
      <xs:annotation>
        <xs:documentation><!-- _locID_textTitle _locComment="" -->A human-friendly title of the package, typically used in UI displays as on nuget.org and the Package Manager in Visual Studio. If not specified, the package ID is used instead.</xs:documentation>
      </xs:annotation>
    </xs:element>
    <xs:element name="PackageLicenseUrl" type="msb:StringPropertyType" substitutionGroup="msb:Property">
      <xs:annotation>
        <xs:documentation><!-- _locID_text="PackageLicenseUrl" _locComment="" -->The URL for the NuGet package's license, often shown in UI displays as well as nuget.org</xs:documentation>
      </xs:annotation>
    </xs:element>
    <xs:element name="PackageProjectUrl" type="msb:StringPropertyType" substitutionGroup="msb:Property">
      <xs:annotation>
        <xs:documentation><!-- _locID_text="PackageProjectUrl" _locComment="" -->The URL for the NuGet package's home page, often shown in UI displays as well as nuget.org</xs:documentation>
      </xs:annotation>
    </xs:element>
    <xs:element name="PackageReleaseNotes" type="msb:StringPropertyType" substitutionGroup="msb:Property">
      <xs:annotation>
        <xs:documentation><!-- _locID_text="PackageReleaseNotes" _locComment="" -->A description of the changes made in this release of the NuGet package, often used in UI like the Updates tab of the Visual Studio Package Manager in place of the package description</xs:documentation>
      </xs:annotation>
    </xs:element>
    <xs:element name="PackageRequireLicenseAcceptance" type="msb:boolean" substitutionGroup="msb:Property">
      <xs:annotation>
        <xs:documentation><!-- _locID_text="PackageRequireLicenseAcceptance" _locComment="" -->Value indicating whether the client must prompt the consumer to accept the NuGet package license before installing the package</xs:documentation>
      </xs:annotation>
    </xs:element>
    <xs:element name="PackageTags" type="msb:StringPropertyType" substitutionGroup="msb:Property">
      <xs:annotation>
        <xs:documentation><!-- _locID_text="PackageTags" _locComment="" -->A space-delimited list of tags and keywords that describe the NuGet package and aid discoverability of NuGet packages through search and filtering mechanisms</xs:documentation>
      </xs:annotation>
    </xs:element>
    <xs:element name="PackageVersion" type="msb:StringPropertyType" substitutionGroup="msb:Property">
      <xs:annotation>
        <xs:documentation><!-- _locID_text="PackageVersion" _locComment="" -->Numeric value of the NuGet package version in the format major.minor.patch pattern (e.g. 1.0.1). Version numbers may include a pre-release suffix (e.g. 1.0.1-beta)</xs:documentation>
      </xs:annotation>
    </xs:element>
    <xs:element name="Product" type="msb:StringPropertyType" substitutionGroup="msb:Property">
      <xs:annotation>
        <xs:documentation><!-- _locID_text="Product" _locComment="" -->Product name information for the assembly manifest</xs:documentation>
      </xs:annotation>
    </xs:element>
    <xs:element name="Version" type="msb:StringPropertyType" substitutionGroup="msb:Property" >
      <xs:annotation>
        <xs:documentation><!-- _locID_text="Version" _locComment="" -->Numeric value of the version in the format major.minor.patch (e.g. 2.4.0)</xs:documentation>
      </xs:annotation>
    </xs:element>
    <xs:element name="VersionPrefix" type="msb:StringPropertyType" substitutionGroup="msb:Property" >
      <xs:annotation>
        <xs:documentation><!-- _locID_text="VersionPrefix" _locComment="" -->When Version is not specified, VersionPrefix represents the first fragment of the version string (e.g. 1.0.0). The syntax is VersionPrefix[-VersionSuffix].</xs:documentation>
      </xs:annotation>
    </xs:element>
    <xs:element name="VersionSuffix" type="msb:StringPropertyType" substitutionGroup="msb:Property">
      <xs:annotation>
        <xs:documentation><!-- _locID_text="VersionSuffix" _locComment="" -->When Version is not specified, VersionSuffix represents the second fragment of the version string (e.g. beta). The syntax is VersionPrefix[-VersionSuffix].</xs:documentation>
      </xs:annotation>
    </xs:element>
    <xs:element name="RestoreSources" type="msb:StringPropertyType" substitutionGroup="msb:Property">
      <xs:annotation>
        <xs:documentation><!-- _locID_text="RestoreSources" _locComment="" -->The feeds that NuGet will use for restoring packages for this project.</xs:documentation>
      </xs:annotation>
    </xs:element>
    <xs:element name="PackageLicenseExpression" type="msb:StringPropertyType" substitutionGroup="msb:Property">
      <xs:annotation>
        <xs:documentation><!-- _locID_text="PackageLicenseExpression" _locComment="" -->The project license's SPDX identifier. Only OSI and FSF approved licenses can use an identifier. Other licenses should use PackageLicenseFile.</xs:documentation>
      </xs:annotation>
    </xs:element>
    <xs:element name="PackageLicenseFile" type="msb:StringPropertyType" substitutionGroup="msb:Property">
      <xs:annotation>
        <xs:documentation><!-- _locID_text="PackageLicenseFile" _locComment="" -->A path to the package's license file. Should only be used when the package doesn't use an OSI or FSF approved license.</xs:documentation>
      </xs:annotation>
    </xs:element>
    <xs:element name="PackageType" type="msb:StringPropertyType" substitutionGroup="msb:Property">
      <xs:annotation>
        <xs:documentation><!-- _locID_text="PackageType" _locComment="" -->Indicates what the intended package use is, e.g. .NET CLI global tool, standard dependency, etc.</xs:documentation>
      </xs:annotation>
    </xs:element>

