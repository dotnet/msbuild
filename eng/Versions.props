    <MicrosoftNETCoreAppPackageVersion>3.0.0-rc2-19457-02</MicrosoftNETCoreAppPackageVersion>
    <MicrosoftDotNetPlatformAbstractionsPackageVersion>3.0.0-rc2-19457-02</MicrosoftDotNetPlatformAbstractionsPackageVersion>
    <MicrosoftExtensionsDependencyModelPackageVersion>3.0.0-rc2-19457-02</MicrosoftExtensionsDependencyModelPackageVersion>
    <MicrosoftNETCoreDotNetHostResolverPackageVersion>3.0.0-rc2-19457-02</MicrosoftNETCoreDotNetHostResolverPackageVersion>
    <MicrosoftNETHostModelVersion>3.0.0-rc2-19457-02</MicrosoftNETHostModelVersion>
  </PropertyGroup>
  <PropertyGroup>
